<header class="page-header">
    <h1>New Calendar</h1>
  </header>
  
  
  <form method="post">
    <h2>Calendar Info</h2>
    <div class="form-group">
      <label for="calendar_name">Name</label>
      <input type="text" class="form-control" name="calendar_name" id="calendar_name">
    </div>
    <div class="form-group">
      <label for="calendar_username">Username</label>
      <input type="text" class="form-control" name="username" id="calendar_username">
    </div>

    <div class="form-group">
      <input type="submit" class="btn btn-primary">
    </div>
  </form>
  
  
  
  
  
  
  
  
  
  
  